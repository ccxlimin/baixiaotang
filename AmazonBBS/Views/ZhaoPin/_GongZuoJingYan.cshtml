@{
    int jingyan = Model;

    var msg2 = string.Empty;
    switch (jingyan)
    {
        case 1:
            msg2 = "不限"; break;
        case 6:
            msg2 = "实习生"; break;
        case 2:
            msg2 = "1-3年"; break;
        case 3:
            msg2 = "3-5年"; break;
        case 4:
            msg2 = "5-10年"; break;
        case 5:
            msg2 = "10年以上"; break;
    }
    <span>@msg2</span>
}